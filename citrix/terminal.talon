tag: user.citrix
-
tag(): user.git

# Commands
remove: "rm "
move: "mv "
make dirk: "mkdir "
extract: "extract "
pan doc: "pandoc "
carol: "curl "
resume: "fg\n"
rerun: key(up enter)
rerun 2: key(up up enter)
cd: "cd "
cd up: "cd ..\n"
edit: "vi "
editor: "vi\n"
list files: "l\n"
resume: "fg\n"
suspended: key(ctrl-z)

dash <user.letters>: " -{letters} "
dash dash <user.word>: " --{word}"
do {user.bash_command}: "{bash_command} "
